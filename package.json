{
  "name": "exhell",
  "version": "1.0.0",
  "description": "AI-powered Excel error correction and automation SaaS",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "build:full": "npx prisma generate && npx prisma migrate deploy && next build",
    "build:safe": "./scripts/safe-build.sh",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "test": "jest",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:init": "node scripts/setup/init-db.js",
    "secrets:generate": "node scripts/setup/generate-secrets.js",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "rollback:staging": "./scripts/rollback.sh staging",
    "rollback:production": "./scripts/rollback.sh production",
    "docker:build": "docker build -t exhell-app .",
    "docker:run": "docker run -p 3000:3000 exhell-app",
    "docker:compose": "docker-compose up -d",
    "docker:compose:test": "docker-compose -f docker-compose.test.yml up -d",
    "docker:compose:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "test:quick": "node scripts/test/quick-test.js",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "socket:dev": "node scripts/utils/socket-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run socket:dev\"",
    "cache:flush": "redis-cli FLUSHDB",
    "start:dev": "./start-dev.sh",
    "sync-knowledge": "tsx scripts/sync-knowledge-data.ts",
    "sync-knowledge:help": "tsx scripts/sync-knowledge-data.ts --help",
    "ai:init": "tsx scripts/initialize-ai-system.ts"
  },
