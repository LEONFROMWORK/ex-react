{
  "name": "exhell",
  "version": "1.0.0",
  "description": "AI-powered Excel error correction and automation SaaS",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "dev:test": "APP_ENV=test NODE_ENV=development next dev",
    "dev:prod": "APP_ENV=production NODE_ENV=development next dev",
    "build": "prisma generate && next build",
    "build:test": "APP_ENV=test npm run build",
    "build:prod": "APP_ENV=production npm run build",
    "start": "next start",
    "start:test": "APP_ENV=test npm run start",
    "start:prod": "APP_ENV=production npm run start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "services": "node scripts/test/start-services.js",
    "services:test": "APP_ENV=test node scripts/test/start-test-services.js",
    "services:prod": "APP_ENV=production node scripts/test/start-prod-services.js",
    "analyze": "ANALYZE=true next build",
    "test": "APP_ENV=test jest",
    "test:unit": "APP_ENV=test jest --testPathPattern=src/.*\\.test\\.ts$",
    "test:integration": "APP_ENV=test jest --testPathPattern=src/.*\\.integration\\.test\\.ts$",
    "test:app": "APP_ENV=test node scripts/test/run-everything.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:test": "DATABASE_URL=file:./test.db prisma migrate dev",
    "db:migrate:prod": "APP_ENV=production prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:init": "node scripts/setup/init-db.js",
    "db:init:test": "APP_ENV=test DATABASE_PROVIDER=sqlite DATABASE_URL=file:./test.db node scripts/setup/init-db.js",
    "db:init:prod": "APP_ENV=production node scripts/setup/init-db.js",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "rollback:staging": "./scripts/rollback.sh staging",
    "rollback:production": "./scripts/rollback.sh production",
    "docker:build": "docker build -t exhell-app .",
    "docker:run": "docker run -p 3000:3000 exhell-app",
    "docker:compose": "docker-compose up -d",
    "docker:compose:test": "docker-compose -f docker-compose.test.yml up -d",
    "docker:compose:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "test:quick": "node scripts/test/quick-test.js",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "socket:dev": "node scripts/utils/socket-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run socket:dev\"",
    "cache:flush": "redis-cli FLUSHDB",
    "start:dev": "./start-dev.sh",
    "env:switch": "node scripts/utils/switch-env.js",
    "env:check": "node scripts/utils/check-env.js",
    "env:restore-test": "node scripts/utils/restore-test-env.js",
    "validate:deploy": "node scripts/utils/validate-deployment.js",
    "git:prepare": "node scripts/utils/prepare-commit.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-s3": "^3.844.0",
    "@azure/storage-blob": "^12.27.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^5.11.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.28.0",
    "@tanstack/react-table": "^8.21.3",
    "@tosspayments/tosspayments-sdk": "^2.3.5",
    "@uploadthing/react": "^6.4.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "exceljs": "^4.4.0",
    "hyperformula": "^3.0.0",
    "ioredis": "^5.6.1",
    "jose": "^5.2.3",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.363.0",
    "nanoid": "^5.1.5",
    "next": "^14.1.4",
    "next-auth": "^5.0.0-beta.16",
    "nodemailer": "^6.10.1",
    "openai": "^4.29.2",
    "react": "^18.2.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.60.0",
    "recharts": "^2.12.3",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sonner": "^2.0.6",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "uploadthing": "^6.8.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.38",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
